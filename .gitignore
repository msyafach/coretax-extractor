!input/sample_*
!input/.gitkeep

output/**/*.png
output/**/*.jpg
output/**/*.jpeg
output/**/*.tiff
output/**/*.bmp
!output/example_*
!output/sample_*
!output/.gitkeep

results/*.xlsx
results/*.xls
!results/example_*
!results/sample_*
!results/.gitkeep

# Log files (but allow example logs)
*.log
!example*.log
!sample*.log

